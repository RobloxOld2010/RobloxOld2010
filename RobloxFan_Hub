-- Create ScreenGui
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "VerificationGui"
screenGui.Parent = game.CoreGui -- Or PlayerGui for local testing

-- Create Frame
local frame = Instance.new("Frame")
frame.Size = UDim2.new(0, 300, 0, 200)
frame.Position = UDim2.new(0.5, -150, 0.5, -100)
frame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
frame.Parent = screenGui
frame.Draggable = true
frame.Active = true

-- Create Title Label
local title = Instance.new("TextLabel")
title.Text = "Verification"
title.Size = UDim2.new(1, 0, 0, 50)
title.BackgroundTransparency = 1
title.Font = Enum.Font.SourceSansBold
title.TextSize = 28
title.TextColor3 = Color3.fromRGB(255, 255, 255)
title.Parent = frame

-- Create TextBox
local codeBox = Instance.new("TextBox")
codeBox.PlaceholderText = "Enter Code Here"
codeBox.Size = UDim2.new(0.8, 0, 0, 30)
codeBox.Position = UDim2.new(0.1, 0, 0.4, 0)
codeBox.Font = Enum.Font.SourceSans
codeBox.TextSize = 20
codeBox.TextColor3 = Color3.fromRGB(0, 0, 0)
codeBox.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
codeBox.Parent = frame

-- Create Enter Button
local enterButton = Instance.new("TextButton")
enterButton.Text = "Enter"
enterButton.Size = UDim2.new(0.5, 0, 0, 30)
enterButton.Position = UDim2.new(0.25, 0, 0.65, 0)
enterButton.Font = Enum.Font.SourceSansBold
enterButton.TextSize = 20
enterButton.BackgroundColor3 = Color3.fromRGB(0, 170, 255)
enterButton.TextColor3 = Color3.fromRGB(255, 255, 255)
enterButton.Parent = frame

-- Create Status Label
local statusLabel = Instance.new("TextLabel")
statusLabel.Text = "Status : On"
statusLabel.Size = UDim2.new(1, 0, 0, 30)
statusLabel.Position = UDim2.new(0, 0, 0.85, 0)
statusLabel.Font = Enum.Font.SourceSans
statusLabel.TextSize = 18
statusLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
statusLabel.BackgroundTransparency = 1
statusLabel.Parent = frame

-- Verification Logic
local correctCode = "901072" -- Change this to your desired code

enterButton.MouseButton1Click:Connect(function()
    if codeBox.Text == "901072" then
        statusLabel.Text = "Verified!"
        statusLabel.TextColor3 = Color3.fromRGB(0, 255, 0)
        wait(1)
       frame.Position = UDim2.new(0, 0, 0.85, 100)
loadstring(game:HttpGet("https://raw.githubusercontent.com/RobloxOld2010/RobloxOld2010/refs/heads/main/Blanks"))()
    else
        statusLabel.Text = "Invalid Code"
        statusLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
    end
end)
